def find_mama_bear_weight(weight1, weight2, weight3):
    weights = [weight1, weight2, weight3]
    weights.sort()  # 重さを昇順でソート
    mama_bear_weight = weights[1]  # ママ ベアのボウルはソート後の中央のボウルです
    return mama_bear_weight

# わかりません
weight1 = int(input())
weight2 = int(input())
weight3 = int(input())

# 関数を呼び出して結果を出力
result = find_mama_bear_weight(weight1, weight2, weight3)
print(result)
