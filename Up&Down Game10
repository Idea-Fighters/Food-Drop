import random

print("컴퓨터가 1~100 중 랜덤 숫자 하나를 정합니다.")
print("이 숫자를 맞춰주세요.")

solution = random.randint(1, 100)

pred = 0
count = 0
while solution != pred:
    pred = int(input("1~100 숫자 입력: "))
    count += 1
    if pred < solution:
        print("DOWN")
    elif pred > solution:
        print("UP")

print(f"정답입니다! {count}회 만에 맞췄어요.")
