def calculate_sum(x, y):
    return x + y

# Input the number of test cases
t = int(input())

# Process each test case
for _ in range(t):
    # Input two integers for each test case
    x, y = map(int, input().split())

    # Calculate and output the sum for each test case
    result = calculate_sum(x, y)
    print(result)
