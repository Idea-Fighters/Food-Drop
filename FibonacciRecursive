public class FibonacciRecursive {
    public static void main(String[] args) {
        int n = 10;
        System.out.println("피보나치 수열의 첫 " + n + "개 항:");
        for (int i = 0; i < n; i++) {
            System.out.print(fibonacci(i) + " ");
        }
    }

    public static int fibonacci(int n) {
        if (n <= 1) {
            return n;
        }
        return fibonacci(n - 1) + fibonacci(n - 2);
    }
}
