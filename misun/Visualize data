import matplotlib.pyplot as plt

# 데이터
years = [2015, 2016, 2017, 2018, 2019, 2020]
sales = [100, 120, 150, 170, 190, 200]

# 선 그래프 그리기
plt.plot(years, sales, marker='o', linestyle='-')

# 그래프 제목과 축 레이블 설정
plt.title('Sales Over Years')
plt.xlabel('Year')
plt.ylabel('Sales')

# x축 눈금 설정
plt.xticks(years)

# 그래프 표시
plt.grid(True)
plt.show()
