a = [3, 7, 2, 6, 9]
b = [6, 8, 9, 4, 3]

score = 0

result = int(input(f'{a[0]} * {b[0]} = ? '))
if result == a[0] * b[0]:
    score += 20
    print("맞았습니다.")
else:
    print("틀렸습니다.")

result = int(input(f'{a[1]} * {b[1]} = ? '))
if result == a[1] * b[1]:
    score += 20
    print("맞았습니다.")
else:
    print("틀렸습니다.")

# 위 6줄을 5번 항목까지 반복합니다.

print("최종 점수는", score, "점입니다.")
