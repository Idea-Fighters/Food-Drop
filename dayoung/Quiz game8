question = ['apple', '물', 'rose', '자동차', 'five']
answer = ['사과', 'water', '장미', 'car', '5']

score = 0

for i in range(5):
    result = input(f'{question[i]} 은(는) 무슨 뜻일까요?')
    if result == answer[i]:
        score += 20
        print("맞았습니다.")
    else:
        print("틀렸습니다.")

print("최종 점수는", score, "점입니다.")

